<article>
	<section><%= count %> : <%= question.content %></section>
	<% replications.forEach(function(replication){ %>
		<% if(replication.question_id == question._id){ %>
		<textarea><%= replication.result %></textarea>
		<% } %>
	<% }); %>
	<button class="answer-text-btn" value="<%= question._id %>">save answer</button>
</article>