<!DOCTYPE html>
<html>
  <head>
    <title></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <script type="text/javascript" src="/javascripts/jquery-1.7.min.js"></script>
    <script type="text/javascript" src="/socket.io/socket.io.js"></script>
    <script type="text/javascript" src="/javascripts/admin_control.js"></script>
  </head>
  <body>
    <% questions.forEach(function(question){ %>
      <% if(question.type == "0"){ %>
        <% include exam_topic_single %>
      <% } %>
      <% if(question.type == "1"){ %>
        <% include exam_topic_double %>
      <%} %>
      <% if(question.type == "2"){ %>
        <% include exam_topic_question %>
      <% } %>
    <% }) %>
  </body>
</html>